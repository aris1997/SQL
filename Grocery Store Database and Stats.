-- Display the database ordered by price.

SELECT * FROM Dami 
ORDER BY price asc;

-- Display the inventory that is less than 10. 

SELECT * FROM DAMI
WHERE inventory <=10 
ORDER BY inventory;

-- What is the SUM of the inventory?

SELECT SUM(inventory) From Dami;
