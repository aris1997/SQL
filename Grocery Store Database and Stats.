-- create a grocery store database

CREATE TABLE Dami (id INTEGER PRIMARY KEY, name TEXT, quantity INTEGER, price INTEGER, aisle INTEGER );

INSERT INTO Dami VALUES (1, "dresses", 20, 10.50, 1 );
